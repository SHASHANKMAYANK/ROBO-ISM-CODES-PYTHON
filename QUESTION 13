y=int(input("Enter the number of elements you want to enter in the array"))
lst = []
for i in range(0,y,1):
    lst.append(int(input("Enter the number to be entered in the array")))
for i in lst:
     if (lst.count(i)%2==1):
         if lst.count(i)>1:
            print(i,"Occurs",lst.count(i),"times in the list which is an odd count")
            break
         else:
             print(i, "Occurs", lst.count(i), "times in the list which is an odd count")
