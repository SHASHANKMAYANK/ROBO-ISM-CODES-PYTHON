x=int(input("Enter the range upto which you want to find prime number\n"))
print("The number 2 is prime")
for i in range(2,x+1,1):
    for j in range(2,i,1):
        if (x==1):
            print("1 is neither prime nor composite")
        elif(i%j==0):
            break
        elif(j==(i-1)):
            print("The number",i,"is prime")
        else:
            continue

